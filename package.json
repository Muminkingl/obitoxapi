{
  "name": "@obitox/upload",
  "version": "1.0.0",
  "type": "module",
  "description": "Simple file upload API - Upload files in 3-7 lines of code with zero bandwidth costs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "file-upload",
    "upload",
    "storage",
    "vercel-blob",
    "aws-s3",
    "cloud-storage",
    "api",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "SubDub Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/subdub-upload.git"
  },
  "homepage": "https://obitox.com",
  "bugs": {
    "url": "https://github.com/yourusername/subdub-upload/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@arcjet/node": "^1.0.0-beta.2",
    "@supabase/supabase-js": "^2.56.1",
    "@upstash/workflow": "^0.2.7",
    "@vercel/blob": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cookie-parser": "~1.4.4",
    "dayjs": "^1.11.13",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "~4.16.1",
    "form-data": "^4.0.5",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.13.0",
    "mongoose": "^8.9.7",
    "morgan": "~1.9.1",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "jest": "^29.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  }
}
